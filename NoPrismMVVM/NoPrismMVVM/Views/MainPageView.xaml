<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="NoPrismMVVM.Views.MainPageView"
             Title="MainPage">
    <StackLayout>
        <Button Command="{Binding Button1}" Text="九条林檎" CommandParameter="九条林檎"/>
        <Button Command="{Binding Button2}" Text="雨ヶ崎笑虹" CommandParameter="雨ヶ崎笑虹"/>
        <Button Command="{Binding Button3}" Text="巻乃もなか" CommandParameter="巻乃もなか"/>
        <Button Command="{Binding Button4}" Text="白乃クロミ" CommandParameter="白乃クロミ"/>
        <Button Command="{Binding Button5}" Text="結目ユイ･･･えっ･･･なにこの子！かわいい！天使！天使か！？" CommandParameter="結目ユイ"/>
        <Label Text="{Binding Label1.Value}" />
        <Button Command="{Binding NextPageButton}" Text="次ページ" />
    </StackLayout>
</ContentPage>